<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".ActivityBase"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/ThemeAds"
        tools:ignore="GoogleAppIndexingWarning">

        <activity
            android:name=".ActivitySplash"
            android:theme="@style/ThemeSplash"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ActivityHome"
            android:label="Home Page"
            android:theme="@style/ThemeDefault" />

        <activity
            android:name=".Ad_Banner"
            android:label="Banner Ads" />

        <activity
            android:name=".Ad_Interstitial"
            android:label="Interstitial Ad" />

        <activity
            android:name=".Ad_Rectangle"
            android:label="Rectangle Ad" />

        <activity
            android:name=".Ad_Reward"
            android:label="Reward Ad" />

        <!--        <activity-->
<!--            android:name=".Ad_Native_List"-->
<!--            android:label="Native Ad List Style" />-->

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-3940256099942544~3347511713"/>

    </application>

</manifest>